{
  "name": "toast",
  "description": "The best place to Stack your JAM",
  "version": "0.2.9",
  "author": "Christopher Biscardi (@chrisbiscardi)",
  "bin": {
    "toast": "./bin/run"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": "https://github.com/ChristopherBiscardi/toast/issues",
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/helper-plugin-utils": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@sector/babel-plugin-package-import": "^0.3.1",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@mdx-js/preact": "^1.5.7",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "babel-plugin-preval": "^4.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "chokidar": "^3.3.1",
    "es-module-lexer": "^0.3.17",
    "globby": "^10",
    "honeycomb-beeline": "^2.0.2",
    "module-alias": "^2.2.2",
    "preact": "^10.3.4",
    "preact-render-to-string": "^5.1.4",
    "react-helmet": "^6.0.0-beta.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/static"
  ],
  "homepage": "https://github.com/ChristopherBiscardi/toast",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "toast",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ChristopherBiscardi/toast",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
